<Window x:Name="mainWin"
		x:Class="ExStorage.Windows.MainWindow"
		xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
		xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
		xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
		xmlns:local="clr-namespace:ExStorage.Windows"
		xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
		xmlns:sys="clr-namespace:System;assembly=mscorlib"
		xmlns:awin="clr-namespace:SharedApp.Windows.ShSupport"
		xmlns:resLocal="clr-namespace:ExStorage.Properties"
		xmlns:resShared="clr-namespace:SharedApp.Windows.ShResources"
		xmlns:supportShared="clr-namespace:SharedApp.Windows.ShSupport"
		xmlns:shExStC="clr-namespace:ShExStorageC.ShSchemaFields.ScSupport"
		xmlns:shExScC="clr-namespace:ShExStorageC.ShSchemaFields"
		xmlns:shExStN="clr-namespace:ShExStorageN.ShExStorage"
		Title="Main Window"
		Height="1050"
		Width="2000"
		DataContext="{Binding RelativeSource={RelativeSource Self}}"
		mc:Ignorable="d"
		Background="{DynamicResource Normal.Window.Background}"
		Foreground="White"
		SnapsToDevicePixels="True"
		Loaded="MainWindow_OnLoaded"
		>

	<Window.Resources>

		<ResourceDictionary>
			<ResourceDictionary.MergedDictionaries>
				<ResourceDictionary Source="SharedSupport/MasterColorList.xaml" />
				<ResourceDictionary Source="SharedSupport/VisualStates.xaml" />
				<ResourceDictionary Source="SharedSupport/CsScrollViewer.xaml" />
				<ResourceDictionary Source="SharedSupport/CsScrollBar.xaml" />
			</ResourceDictionary.MergedDictionaries>

			<sys:Double x:Key="PanelWidth01">400</sys:Double>
			<sys:Double x:Key="TitleWidth01">170</sys:Double>

			<sys:Double x:Key="TitleWidth02a">70</sys:Double>
			<sys:Double x:Key="TitleWidth02b">85</sys:Double>

			<sys:Double x:Key="PanelWidth02">190</sys:Double>

			<sys:Double x:Key="BtnHeight">20.0</sys:Double>
			<sys:Double x:Key="BtnWidth">110.0</sys:Double>

			<SolidColorBrush x:Key="TbxBorder" Color="{StaticResource True.Blue.300}" />

			<!-- <shExStC:PassThruConverter x:Key="PassThru" /> -->
			<shExStC:KeyToFieldValueConverter x:Key="KeyToField" />
			<!-- <shExStC:KeyToFieldValueConverter x:Key="KeyToValue" /> -->
			<!-- <shExStC:KeyToFieldNameConverter x:Key="KeyToFieldName" /> -->


			<Style x:Key="tblk.title01"
					TargetType="TextBlock"
					>
				<Setter Property="Width" Value="{StaticResource TitleWidth02b}" />
				<Setter Property="HorizontalAlignment" Value="Left" />
				<Setter Property="TextAlignment" Value="Right" />
				<Setter Property="Margin" Value="0,0,5,0">
				</Setter>
			</Style>

			<Style x:Key="BtnBaseStyle"
					TargetType="Button"
					BasedOn="{StaticResource Button.Normal.Style}"
					>
				<Setter Property="Height" Value="{StaticResource BtnHeight}" />
				<Setter Property="Width" Value="{StaticResource BtnWidth}" />
			</Style>

			<Style x:Key="BtnBottomRow"
					TargetType="Button"
					BasedOn="{StaticResource BtnBaseStyle}"
					>
				<Setter Property="Margin" Value="3,0,3,0" />
			</Style>

			<Style x:Key="BtnOps"
					TargetType="Button"
					BasedOn="{StaticResource BtnBaseStyle}"
					>
				<Setter Property="Margin" Value="0,2,0,0" />
			</Style>

			<Style x:Key="BdrStkPnl"
					TargetType="Border"
					>
				<Setter Property="BorderBrush" Value="White" />
				<Setter Property="BorderThickness" Value="0,2,0,0.5" />
				<Setter Property="Margin" Value="3,0,3,0" />
			</Style>

			<Style x:Key="BdrTxBlkHdrBottLine"
					TargetType="Border"
					>
				<Setter Property="Padding" Value="0,1,0,2" />
				<Setter Property="BorderBrush" Value="White" />
				<Setter Property="BorderThickness" Value="0,0,0,0.5" />
			</Style>

			<Style x:Key="BdrTxBlkHdrTnBLine"
					TargetType="Border"
					>
				<Setter Property="Padding" Value="0,1,0,2" />
				<Setter Property="BorderBrush" Value="White" />
				<Setter Property="BorderThickness" Value="0,2,0,0.5" />
			</Style>


			<Style x:Key="FocusVisual">
				<Setter Property="Control.Template">
					<Setter.Value>
						<ControlTemplate>
							<Rectangle
									Margin="2"
									StrokeDashArray="1 2"
									SnapsToDevicePixels="true"
									StrokeThickness="1"
									Stroke="{DynamicResource {x:Static SystemColors.ControlTextBrushKey}}"
									/>
						</ControlTemplate>
					</Setter.Value>
				</Setter>
			</Style>

			<Style x:Key="ShtkStackedBtn"
					TargetType="TextBlock"
					>
				<Setter Property="FontSize" Value="12" />
				<Setter Property="Margin" Value="0" />
				<Setter Property="Padding" Value="0" />
				<Setter Property="HorizontalAlignment" Value="Center" />

			</Style>

		</ResourceDictionary>

	</Window.Resources>

	<Grid Margin="3">
