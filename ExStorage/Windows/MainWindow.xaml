			</Grid.RowDefinitions>


			<ListBox
					Grid.Column="0"
					Grid.Row="0"
					Margin="0,0,5,15"
					HorizontalAlignment="Stretch"
					Background="Transparent"
					DataContext="{Binding MwModel.SheetDataCurrent.Fields}"
					ItemsSource="{Binding Values}"
					d:ItemsSource="{d:SampleData}"
					HorizontalContentAlignment="Stretch"
					>
				<ListBox.ItemTemplate>
					<DataTemplate>
						<DockPanel
								Dock="top"
								HorizontalAlignment="Stretch"
								>

							<TextBlock
									DockPanel.Dock="Left"
									Width="110"
									Foreground="White"
									Text="{Binding Path=Meta1Field.FieldName}"
									/>

							<TextBox
									DockPanel.Dock="Right"
									Text="{Binding Path=Value}"
									/>

						</DockPanel>

					</DataTemplate>

				</ListBox.ItemTemplate>
			</ListBox>


			<ListBox
					Grid.Column="0"
					Grid.Row="1"
					HorizontalAlignment="Stretch"
					Background="Transparent"
					DataContext="{Binding MwModel.SheetDataCurrent.Rows}"
					ItemsSource="{Binding Values}"
					d:ItemsSource="{d:SampleData ItemCount=6}"
					HorizontalContentAlignment="Stretch"
					ScrollViewer.VerticalScrollBarVisibility="Visible"
					ScrollViewer.HorizontalScrollBarVisibility="Visible"
					ScrollViewer.CanContentScroll="False"
					>
				<ListBox.ItemTemplate>

					<DataTemplate DataType="{x:Type shExScC:ScDataRow}">
						<StackPanel Orientation="Vertical">
							<TextBlock
									Text="{Binding Path=SchemaName}"
									Foreground="White"
									/>

							<ListBox
									DataContext="{Binding RelativeSource={RelativeSource AncestorType={x:Type StackPanel}}, Path=DataContext}"
									ItemsSource="{Binding Path=Fields}"
									Background="Blue"
									HorizontalContentAlignment="Stretch"
									>
								<ListBox.ItemTemplate>
									<DataTemplate>
										<DockPanel HorizontalAlignment="Stretch">
											<TextBlock
													Width="110"
													Text="{Binding Path=Value.FieldName}"
													Foreground="White"
													/>
											<TextBox
													Text="{Binding Path=Value.Value}"
													HorizontalAlignment="Stretch"
													/>

										</DockPanel>
									</DataTemplate>
								</ListBox.ItemTemplate>

							</ListBox>
						</StackPanel>
					</DataTemplate>


				</ListBox.ItemTemplate>
			</ListBox>


